  "license": "MIT",
  "scripts": {
    "build": "tsc && sass scss:./ && webpack --mode production --config webpack.config.js",
    "start": "tsc && webpack-dev-server --mode development --config webpack.development.js",
    "serve:production": "http-server",
    "watch": "concurrently \"tsc --watch\" \"sass scss:./ --watch\" \"webpack --watch --mode development --config webpack.development.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "http-server": "^0.11.1",
    "sass": "^1.18.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.3"
  }
}
